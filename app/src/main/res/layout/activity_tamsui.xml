<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.myapplication_pethospital.TamsuiActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp"
        android:weightSum="1">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.76"
            android:text="@string/textView200" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:orientation="horizontal"
            android:layout_weight="0.01">

            <TextView
                android:id="@+id/textView_City_T"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:text="@string/textView_City_T" />

            <TextView
                android:id="@+id/textView_T"
                android:layout_width="wrap_content"
                android:layout_height="49dp"
                android:layout_weight="1"
                android:text="@string/textView_T" />

        </LinearLayout>

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="284dp">

            <RadioButton
                android:id="@+id/radioButton_T1"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:text="@string/radioButton_T1" />

            <RadioButton
                android:id="@+id/radioButton_T2"
                android:layout_width="match_parent"
                android:layout_height="51dp"
                android:text="@string/radioButton_T2" />

            <RadioButton
                android:id="@+id/radioButton_T3"
                android:layout_width="match_parent"
                android:layout_height="54dp"
                android:text="@string/radioButton_T3" />

            <RadioButton
                android:id="@+id/radioButton_T4"
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:text="@string/radioButton_T4" />

            <RadioButton
                android:id="@+id/radioButton_T5"
                android:layout_width="match_parent"
                android:layout_height="52dp"
                android:text="@string/radioButton_T5" />
        </RadioGroup>

        <Button
            android:id="@+id/button_return_Main"
            android:layout_width="189dp"
            android:layout_height="wrap_content"
            android:text="@string/button_return_M" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/textView170" />

        <Button
            android:id="@+id/button_enter_T"
            android:layout_width="194dp"
            android:layout_height="wrap_content"
            android:text="@string/button_enter_T" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
